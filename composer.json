{
    "name": "derrabus/ereg-shim",
    "description": "Replacement for the ereg legacy php extension.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Rabus\\EregShim\\": "src"
        },
        "files": [ "src/bootstrap.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rabus\\EregShim\\": "tests"
        }
    },
    "require": {
        "php": "^5.3 || ^7 || ^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.6 || ^6 || ^7 || ^8 || ^9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander M. Turek",
            "email": "me@derrabus.de"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
